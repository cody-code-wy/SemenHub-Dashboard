<%= link_to 'Add Registration', new_registration_path %>
<table>
  <thead>
  <tr>
    <th>Registrar</th>
    <th class="nosort">Registration Info</th>
    <th>Registration</th>
    <th>Note</th>
  </tr>
  </thead>
  <tbody>
  <% @regs.each do |reg| %>
      <tr>
        <td><%= reg.registrar.name %></td>
        <td><%= link_to 'Information', reg %></td>
        <td><%= reg.registration %></td>
        <td><%= reg.note %></td>
      </tr>
  <% end %>
  </tbody>
</table>
