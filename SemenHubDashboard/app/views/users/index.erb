<%= link_to "Create User", new_user_path %>
<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Spouse Name</th>
      <th>Email Address</th>
      <th>Primary Phone</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to "Show Profile", user %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.spouse_name %></td>
        <td><%= user.email %></td>
        <td><%= user.phone_primary %></td>
      </tr>
    <% end %>
  </tbody>
</table>
