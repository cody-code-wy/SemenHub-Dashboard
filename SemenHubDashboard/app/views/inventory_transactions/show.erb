<% if @transaction.private %>
  <h3>Marked as private</h3>
<% end %>
<table>
  <thead>
    <tr>
      <th>Price per unit</th>
      <th>Semen type</th>
      <th>Semen count</th>
      <th>Animal</th>
      <th>Animal Owner</th>
      <th>Storage Facility</th>
      <th>Seller</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @transaction.price_per_unit %></td>
      <td><%= ["Conventional","Male Sexed","Female Sexed"][@transaction.semen_type] %></td>
      <td><%= ["2.1","5.0"][@transaction.semen_count] %></td>
      <td><%= @transaction.animal.name %></td>
      <td><%= @transaction.animal.owner.get_name %></td>
      <td><%= @transaction.storageFacility.name %></td>
      <td><%= @transaction.seller.get_name %></td>
    </tr>
  </tbody>
</table>
Transactions
<table>
  <thead>
    <th>Transaction ID</th>
    <th>Quantity</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th></th>
  </thead>
  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.id %></td>
        <td><%= transaction.quantity %></td>
        <td><%= transaction.created_at %></td>
        <td><%= transaction.updated_at %></td>
        <td><%= link_to "edit", edit_inventory_transaction_path(transaction) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
